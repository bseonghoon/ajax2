<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.naver.homefood2.mapper.SignMapper">

	<insert id="signUp" parameterType="com.naver.homefood2.vo.User">
		INSERT
		INTO user(
		id
		,passwd
		,type
		,email
		,location
		)
		VALUES(
		#{id}
		,#{password}
		,#{type}
		,#{email}
		,#{location}
		)
	</insert>
</mapper>